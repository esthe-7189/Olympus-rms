		<ITEM SRC="images/image06.jpg" LINK="../product/product.jsp" POPUP="false" />
		<ITEM SRC="images/image07.jpg" LINK="../product/product.jsp" POPUP="false" />
		<ITEM SRC="images/image08.jpg" LINK="../product/product.jsp" POPUP="false" />		
	</ITEMS>
