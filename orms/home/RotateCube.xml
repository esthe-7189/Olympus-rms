<?xml version="1.0" encoding="utf-8"?>
<DATA>
	
	<!-- 환경설정 설정 -->
	<CONFIG>
	<!-- 큐브 -->
		<!-- 큐브 너비 (pixel) -->
		<CUBE_WIDTH  VALUE="400" />
		<!-- 큐브 높이 (pixel) -->
		<CUBE_HEIGHT  VALUE="240" />
		<!-- 큐브 색상 (ex:FFFFFF) -->
		<CUBE_COLOR  VALUE="545758" />
		
	<!-- 배경 -->	
		<!-- 배경 이미지 경로 (URL) -->
		<BACKGROUND_URL  VALUE="images/background.jpg" />
		<!-- 배경 색상 (ex:FFFFFF) -->
		<BACKGROUND_COLOR  VALUE="FFFFFF" />	
		
	<!-- 자동 슬라이드 -->	
		<!-- 자동 이동 사용 여부 (사용:true, 미사용:false) -->
		<AUTOSLIDE_USE 	VALUE="true" />		
		<!-- 자동이동 시간 (초) -->
		<AUTOSLIDE_INTERVAL  VALUE="3" />		
		
	<!-- 효과 -->	
		<!-- 큐브 회전 방향 (가로:HORIZONTAL, 세로:VERTICAL)  -->
		<TWEEN_DIRECTTION  VALUE="HORIZONTAL" />	
		<!-- 큐브 회전 시간 (초) -->
		<TWEEN_DURATION   VALUE="0.6" />		
		<!-- 큐브 회전 효과 ( 메뉴얼 참고 ) -->
		<TWEEN_EASE  VALUE="" />			
		<!-- 시작 효과 사용여부 (사용:true, 미사용:false) -->
		<TWEEN_INTRO_USE  VALUE="true" />		
		<!-- 효과 중복 사용 여부 (사용 : true, 미사용 : false) -->
		<TWEEN_OVERLAP_USE  VALUE="true"/>

	<!-- 반사 효과 -->
		<!-- 반사 효과 사용 여부 (사용:true, 미사용:false) -->
		<REFLECTION_USE  VALUE="true" />
		<!-- 반사 투명도 (불투명 0 ~ 100 투명) -->
		<REFLECTION_ALPHA  VALUE="40" />
		<!-- 반사 비율 (짧게 0~10 길게) --> 
		<REFLECTION_RATIO  VALUE="7" />
		
	<!-- 프레임 -->	
		<!-- 프레임 적용 여부 (사용:true, 미사용:false) -->
		<FRAME_USE  VALUE="true" />	
		<!-- 프레임 여백 (pixel) -->
		<FRAME_PADDING  VALUE="4" />
		<!-- 프레임 배경 색상 (ex:FFFFFF) -->
		<FRAME_COLOR  VALUE="FFFFFF" />	
		<!-- 프레임 테두리 색상 (ex:000000) -->
		<FRAME_BORDER_COLOR  VALUE="000000" />			
		<!-- 프레임 테두리 두께 (pixel) -->
		<FRAME_BORDER_THICK  value="1" />		
		
	<!-- 버튼 -->	
		<!-- 이동버튼 사용 여부 (사용:true, 미사용:false) -->	
		<BUTTON_USE  VALUE="true" />
		<!-- 이전 버튼 이미지 경로 -->
		<BUTTON_PREV_URL  VALUE="images/prev.png" />	
		<BUTTON_PREV_OVER_URL  VALUE="images/prev_over.png" />
		<BUTTON_PREV_DOWN_URL  VALUE="images/prev_over.png" />
		<!-- 이전 버튼 위치 (-1이면 화면 중앙) -->
		<BUTTON_PREV_TOP  VALUE="-1" />
		<BUTTON_PREV_LEFT  VALUE="95" />
		<!-- 다음 버튼 이미지 경로 -->
		<BUTTON_NEXT_URL  VALUE="images/next.png"/>
		<BUTTON_NEXT_OVER_URL  VALUE="images/next_over.png"/>
		<BUTTON_NEXT_DOWN_URL  VALUE="images/next_over.png"/>
		<!-- 다음 버튼 위치 (-1이면 화면 중앙)-->
		<BUTTON_NEXT_TOP  VALUE="-1" />
		<BUTTON_NEXT_LEFT  VALUE="786" />
		
	<!-- 링크 -->
		<!-- 링크 사용 여부 (사용:true, 미사용:false) -->
		<LINK_USE  VALUE="true" />
		<!-- 링크에 마우스가 올라갔을 떄 손가락 모양으로 변경 여부 -->
		<LINK_HANDCURSOR_USE  VALUE="true" />
		<!-- 링크 클릭시 자바스크립트로 이벤트를 받고 싶을 경우 자바스크립트 함수 이름 -->
		<LINK_CLICK_HANDLER  VALUE="onLinkClick" />
	</CONFIG>
		
	<!-- 아이템 목록 -->
	<ITEMS>
		<!--  SRC - 이미지 경로 (필수) -->
		<!--  LINK – 큐브 클릭시 이동할 경로, 공백을 입력하시면 이동하지 않음 -->
		<!--POPUP – 큐브 클릭시 새 창을 띄워 링크로 이동할지 여부, 사용안함(false)면 현재 페이지에서 이동 -->
		<ITEM SRC="images/image01.jpg" LINK="../product/product.jsp" POPUP="false" />
		<ITEM SRC="images/image02.jpg" LINK="../product/product.jsp" POPUP="false" />
